@article{ellaway1978,
title = {Cumulative sum technique and its application to the analysis of peristimulus time histograms},
journal = {Electroencephalography and Clinical Neurophysiology},
volume = {45},
number = {2},
pages = {302-304},
year = {1978},
issn = {0013-4694},
doi = {https://doi.org/10.1016/0013-4694(78)90017-2},
url = {https://www.sciencedirect.com/science/article/pii/0013469478900172},
author = {P.H Ellaway},
abstract = {The cumulative sum procedure introduced by Hurst (1950) involves subtraction of a control reference level from a series of datum points and adding the differences consecutively. The resultant curve shows trends indicating changed levels of points. The techniques has been applied to peristimulus time histograms in order to reveal small changes in the probability of spike occurrences normally obscured by random fluctuations. The combined process of cumulative sum and PSTH cuts the overall number of trials needed in real time and reduces the uncertainty of decision making during the experiment.
Résumé
La technique de l'addition cumulative introduite par Hurst (1950) consiste à soustraire d'une série de valeurs ponctuelles, une certaine valeur de référence et à cumuler les différences successives ainsi obtenues. La courbe résultante indique les tendances de variation du niveau des points. La méthode est appliquée ici aux histogrammes séquentiels péristimulus, en vue de déceler de faibles changements dans la probabilité de survenue d'une spike, normalement masquée par des fluctuations aléatoires. La combinaison des méthodes de l'addition cumulative et du PSTH diminue le nombre d'épreuves nécessaires en temps réel et réduit l'incertitude dans la prise d'une décision au cours de l'expérience.}
}

