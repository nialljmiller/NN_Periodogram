title = {{FOLLOW UP OBSERVATIONS OF SDSS AND CRTS CANDIDATE CATACLYSMIC VARIABLES *}},
url = {http://ooruri.kusastro.kyoto-u.ac.jp/pipermail/vsnet-alert/},
